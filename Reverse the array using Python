def reverse(arr,start, end):
    while start<end:
        arr[start], arr[end]=arr[end], arr[start]
        start=start+1
        end=end-1
        
n=int(input("Enter the array size: "))
arr=[]
print("Enter the array element: ")
for i in range(0,n):
    ele=int(input())
    arr.append(ele)
print(arr)
reverse(arr,0,n-1)
print(arr)
